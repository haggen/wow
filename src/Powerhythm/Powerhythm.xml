<Ui xmlns="http://www.blizzard.com/wow/ui/" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Script file="Powerhythm.lua"/>
	<Frame name="PowerhythmFrame" parent="UIParent" enableMouse="true" movable="true" hidden="false">
		<Size x="32" y="32"/>
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<Texture name="$parentTexture" setAllPoints="true">
					<Color r="0.0" g="0.0" b="0.0" a="0.5"/>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad function="PowerhythmFrame_OnLoad"/>
			<OnEvent function="PowerhythmFrame_OnEvent"/>
			<OnDragStart function="PowerhythmFrame_OnDragStart"/>
			<OnDragStop function="PowerhythmFrame_OnDragStop"/>
			<OnUpdate function="PowerhythmFrame_OnUpdate"/>
		</Scripts>
	</Frame>
</Ui>
